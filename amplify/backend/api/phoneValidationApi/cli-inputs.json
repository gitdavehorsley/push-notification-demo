{
  "version": 1,
  "paths": {
    "/verify": {
      "name": "/verify",
      "lambdaFunction": "pushnotificationdemo1e717369",
      "permissions": {
        "setting": "open"
      },
      "cors": {
        "origins": ["*"],
        "methods": ["OPTIONS", "POST"],
        "headers": [
          "Content-Type",
          "Authorization",
          "Access-Control-Allow-Origin",
          "Access-Control-Allow-Headers",
          "Access-Control-Allow-Methods"
        ],
        "allowCredentials": false,
        "maxAge": 300
      }
    }
  }
}
